{
  "build":[
    {"src": "app.py", "use": "@vercel/python"}
  ],
  "functions": {
    "message": {
      "runtime": "vercel-php@0.5.2"
    },
    "call": {
      "runtime": "vercel-php@0.5.2"
    }
  },
  "routes": [
    { "src": "/message", "dest": "/api/message" },
    { "src": "/call", "dest": "/api/call" }
  ]
}
